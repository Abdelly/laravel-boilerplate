{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 2%"],
        "uglify": true
      }
    }]
  ],
  "plugins": [
    "lodash",
    "transform-object-rest-spread",
    [
      "transform-runtime", {
        "polyfill": false,
        "helpers": false
      }
    ]
  ]
}
