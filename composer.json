{
    "name": "adr1enbe4udou1n/laravel-boilerplate",
    "description": "Lite boilerplate site with backend based on Laravel 5.4.",
    "keywords": ["framework", "laravel", "boilerplate", "adminlte"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "ext-gd": "*",
        "arcanedev/no-captcha": "^4.0",
        "artesaos/seotools": "^0.10.0",
        "cviebrock/eloquent-sluggable": "^4.2",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "dimsav/laravel-translatable": "^7.2",
        "doctrine/annotations": "1.4.*",
        "doctrine/cache": "1.6.*",
        "doctrine/collections": "1.4.*",
        "doctrine/common": "2.7.*",
        "doctrine/dbal": "2.5.*",
        "doctrine/instantiator": "1.0.*",
        "guzzlehttp/guzzle": "^6.2",
        "hieu-le/active": "^3.3",
        "intervention/image": "^2.3",
        "intervention/imagecache": "^2.3",
        "laravel/dusk": "^1.1",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maatwebsite/excel": "^2.1",
        "martinlindhe/laravel-vue-i18n-generator": "^0.1.16",
        "mcamara/laravel-localization": "^1.2",
        "mews/purifier": "^2.0",
        "plank/laravel-mediable": "^2.4",
        "roumen/sitemap": "^2.6",
        "tightenco/ziggy": "^0.3.0",
        "yajra/laravel-datatables-oracle": "^7.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.2",
        "fzaninotto/faker": "~1.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "mockery/mockery": "0.9.*",
        "phpro/grumphp": "^0.11.6",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
